bool avoidObstacle(){
  const double alarmDist=30;
  int weewoo=0;
  while(weewoo<3){
    double dist= Distance2.objectDistance(mm);
    if(dist>0 && dist <alarmDist){
      Brain.Screen.print("Obstacle Detected");
      Brain.Screen.newLine();
      Brain.Screen.print(dist);
      Brain.Screen.print("Away!");

      brain.playSound(soundType::alarm);
      weewoo++;
      wait(5,seconds);
    }
    else{
      return true;
    }
  }
  return false;
